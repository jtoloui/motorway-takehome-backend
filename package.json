{
  "name": "motorway-takehome-backend",
  "version": "1.0.0",
  "description": "This service allows you to lookup a given vehicle based on a time frame to see it's seller state history",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "node dist/bundle.js",
    "dev": "TZ=UTC NODE_ENV=development concurrently \"webpack --config webpack.config.js\" \"nodemon --watch dist --exec node dist/bundle.js\"",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "generate:types": "kanel",
    "test": "TZ=UTC jest --detectOpenHandles --forceExit",
    "test:watch": "TZ=UTC jest --watch --detectOpenHandles",
    "swagger-autogen": "node swagger.js",
    "prettier": "prettier --check \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\""
  },
  "keywords": [],
  "author": "Jamie Toloui",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "memjs": "^1.3.2",
    "pg": "^8.11.5",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/memjs": "^1.3.3",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "kanel": "^3.8.9",
    "kanel-zod": "^1.3.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.9.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
